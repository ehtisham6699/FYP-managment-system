<% include ../partials/navbar %>
<div class="container-fluid">
    <div class="row">
    <% include ../partials/side_nav %>
    <main role="main" class="col-md-9 ml-sm-auto col-lg-10 pt-3 px-4">
        <div class="d-flex justify-content-between flex-wrap flex-md-nowrap align-items-center pb-2 mb-3 border-bottom">
          <h1 class="h2">meetings</h1>
          <% include ../partials/messages %>
        </div>
       
        <table class="table table-bordered" width="300" cellspacing="0" cellpadding="0">
            <thead>
              <tr>
               
                <th scope="col" style="width: 15%">meeting Week</th>
                <th scope="col" style="width: 10%">Date</th>
                <th scope="col" style="width: 30%">discusion</th>
                <th scope="col" style="width: 15%">status</th>
                <th scope="col" style="width: 15%">actions</th>
                
              </tr>
            </thead>
            <% for(var i=0; i<data.meetinglogs.length; i++) {%>
            <tbody>
              <tr>
                <th scope="row"><%= data.meetinglogs[i].meetingname %></th>
                <td><%= data.meetinglogs[i].date %></td>
                <td><%= data.meetinglogs[i].description %></td>
                <td><%= data.meetinglogs[i].approved %></td>
                <td>
                    <form action="/groups/onclickmeeting" method="post">
                        <input type="text"name="gid" hidden value="<%= data._id %>">
                        <input type="text"name="logid" hidden value="<%= data.meetinglogs[i]._id %>">
                        <button class=" btn btn-primary">approve</button>
                    </form>
                   
                </td>
                
              </tr>
              
             
            </tbody>
            <% } %> 
          </table>
            
    </main>
    </div>